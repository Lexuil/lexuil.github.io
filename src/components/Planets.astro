---
import Planet from "../components/Planet.astro";
import mars from "../assests/images/mars.png"
import other from "../assests/images/other.png"
---

<Planet
  class="w-96"
  imageUrl={mars}
  xPosition="-20vw"
  top="8vh"
  animationTime={6}
/>
<Planet
  class="w-40 md:w-60 brightness-[0.3]"
  imageUrl={other}
  xPosition="15vw"
  top="35vh"
  animationTime={4}
/>
<Planet
  class="w-28 md:w-52 brightness-[0.3]"
  imageUrl={other}
  xPosition="-10vw"
  top="60vh"
  animationTime={5}
/>